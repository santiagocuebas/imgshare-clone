<div class="container-user-head">
	<div class="user-name">
		<div class="user-img"><img src="/uploads/avatars/default.png"></div>
			<div>
				<h1 class="user-title">{{user.username}}</h1>
				<div>
					{{#if user}}
					<a class="user-link" href=""><i class="fa-solid fa-file-arrow-up"></i> UPLOAD</a>
					{{/if}}
					<a class="user-link" href=""><i class="fa-solid fa-comments"></i> CHAT</a>
				</div>
			</div>
		</div>
		<div class="user-list">
			<a class="user-link" href="">POST</a>
			<a class="user-link" href="">COMMENT</a>
			<a class="user-link" href="">ABOUT</a>
		</div>
	</div>
</div>
<div class="container">
	<div class="container-gallery">
		<div></div>
		<div class="gallery">
			{{#each images}}
			<div class="box">
				<a href="/gallery/{{uniqueId}}" class="gallery-link">
					<div class="gallery-text"><h2>{{title}}</h2></div>
				</a>
				<img src="/uploads/{{filename}}" class="gallery-image">
			</div>
			{{/each}}
		</div>
	</div>
</div>